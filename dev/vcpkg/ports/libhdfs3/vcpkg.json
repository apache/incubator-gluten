{
  "name": "libhdfs3",
  "version-date": "2019-11-05",
  "port-version": 3,
  "description": "Native Hadoop RPC protocol and HDFS data transfer protocol implementation",
  "homepage": "https://github.com/erikmuttersbach/libhdfs3",
  "supports": "!windows",
  "dependencies": [
    "boost-thread",
    "boost-chrono",
    "boost-system",
    "boost-atomic",
    "boost-iostreams",
    "boost-function",
    "boost-bind",
    "boost-crc",
    "boost-functional",
    {
      "name": "libuuid",
      "platform": "!windows & !osx"
    },
    "libxml2",
    "protobuf",
    "krb5",
    "gsasl",
    {
      "name": "yasm",
      "host": true,
      "features": [
        "tools"
      ]
    }
  ]
}
